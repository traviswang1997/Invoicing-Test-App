AA,M,MAAA,CAGG,CAJL,C,CAHF,C;GAQK,CARL,E;;AAAA,GASM,EAVR,CAWM,CAjBR,CAkBI,CA3BN,C","sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <form @submit.prevent>\r\n      <label for=\"invoices\">Create a new invoice</label>\r\n      <input type=\"text\" name=\"invoices\" v-model=\"state.description\" placeholder=\"Description\" />\r\n      <button @click=\"createInvoice\">Create Invoice</button>\r\n    </form>\r\n\r\n    <hr />\r\n\r\n    <table>\r\n      <thead>\r\n        <th>ID</th>\r\n        <th>Description</th>\r\n        <th></th>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"invoice in state.invoices\" :key=\"invoice.id\">\r\n          <td>{{invoice.id}}</td>\r\n          <td>{{invoice.description}}</td>\r\n          <td>\r\n            <router-link :to=\"{ name: 'Invoice', params: { id: invoice.id }}\">\r\n              Open\r\n            </router-link>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { defineComponent, onMounted, reactive } from 'vue';\r\n// @ is an alias to /src\r\n\r\nexport default defineComponent({\r\n  name: 'Invoices',\r\n  setup() {\r\n    const state = reactive({\r\n      invoices: [],\r\n      description: \"\"\r\n    })\r\n\r\n    function fetchInvoices() {\r\n      fetch(\"http://localhost:5000/invoices\", {\r\n        method: \"GET\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\"\r\n        },\r\n      }).then((response) => {\r\n        response.json().then(invoices => (state.invoices = invoices))\r\n      })\r\n    }\r\n\r\n    function createInvoice() {\r\n      fetch(\"http://localhost:5000/invoices\", {\r\n        method: \"PUT\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\"\r\n        },\r\n        body: JSON.stringify({\r\n          description: state.description\r\n        })\r\n      }).then(fetchInvoices)\r\n    }\r\n\r\n    onMounted(fetchInvoices)\r\n\r\n    return {state, createInvoice}\r\n  }\r\n});\r\n</script>\r\n"],"sourceRoot":""}]}