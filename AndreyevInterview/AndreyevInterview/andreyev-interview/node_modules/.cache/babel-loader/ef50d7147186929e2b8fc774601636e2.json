A8F;AAAvF,IAAA,IAAI,EAAC,MAAkF;AAA3E,IAAA,IAAI,EAAC,aAAsE;AAAxD,IAAA,WAAW,EAAC,aAA4C;;aAArB,IAAA,CAAA,KAAA,CAAM,WAAN,GAAiB,M;;AAAI,GAA9F,E,IAAA,E;;AAAA,G,iBAAyE,IAAA,CAAA,KAAA,CAAM,W,qBAC/E,YAAA,CAAuF,OAAvF,EAAuF;AAAhF,IAAA,IAAI,EAAC,QAA2E;AAAlE,IAAA,IAAI,EAAC,UAA6D;AAAlD,IAAA,WAAW,EAAC,UAAsC;;aAAlB,IAAA,CAAA,KAAA,CAAM,QAAN,GAAc,M;;AAAI,GAAvF,E,IAAA,E;;AAAA,G,iBAAqE,IAAA,CAAA,KAAA,CAAM,Q,qBAC3E,YAAA,CAA2E,OAA3E,EAA2E;AAApE,IAAA,IAAI,EAAC,QAA+D;AAAtD,IAAA,IAAI,EAAC,MAAiD;AAA1C,IAAA,WAAW,EAAC,MAA8B;;aAAd,IAAA,CAAA,KAAA,CAAM,IAAN,GAAU,M;;AAAI,GAA3E,E,IAAA,E;;AAAA,G,iBAA6D,IAAA,CAAA,KAAA,CAAM,I,KACnE,YAAA,CAAuD,QAAvD,EAAuD;AAA9C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,IAAA,CAAA,cAAA,IAAA,IAAA,CAAA,cAAA,OAAA,IAAA,YAAF;AAAA,KAAA;AAAyC,GAAvD,EAAgC,gBAAhC,C,CALF,E;;AAAA,GAOI,CAjCN,C","sourcesContent":["<template>\r\n  <div class=\"about\">\r\n    <router-link :to=\"{ name: 'Invoices' }\">Back</router-link>\r\n\r\n    <h2>Invoice Details</h2>\r\n\r\n    <span>Invoice #{{$route.params.id}}</span>\r\n\r\n    <h3>Line Items</h3>\r\n\r\n    <table>\r\n      <thead>\r\n        <th>ID</th>\r\n        <th>Description</th>\r\n        <th>Quantity</th>\r\n        <th>Cost</th>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"item in state.lineItems\" :key=\"item.id\">\r\n          <td>{{item.id}}</td>\r\n          <td>{{item.description}}</td>\r\n          <td>{{item.quantity}}</td>\r\n          <td>{{item.cost}}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <form @submit.prevent>\r\n      <h4>Create Line Item</h4>\r\n      <input type=\"text\" name=\"description\" placeholder=\"Description\" v-model=\"state.description\" />\r\n      <input type=\"number\" name=\"quantity\" placeholder=\"Quantity\" v-model=\"state.quantity\" />\r\n      <input type=\"number\" name=\"cost\" placeholder=\"Cost\" v-model=\"state.cost\" />\r\n      <button @click=\"createLineItem\">Create Invoice</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport {defineComponent, onMounted, reactive} from \"vue\";\r\n\r\nexport default defineComponent({\r\n  name: \"Invoice\",\r\n  props: {\r\n    id: {\r\n      type: [String, Number],\r\n      required: true\r\n    }\r\n  },\r\n  setup(props) {\r\n    const state = reactive({\r\n      lineItems: [],\r\n      description: \"\",\r\n      quantity: \"0\",\r\n      cost: \"0\"\r\n    })\r\n\r\n    function fetchLineItems() {\r\n      fetch(`http://localhost:5000/invoices/${props.id}`, {\r\n        method: \"GET\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\"\r\n        },\r\n      }).then((response) => {\r\n        response.json().then(lineItems => (state.lineItems = lineItems))\r\n      })\r\n    }\r\n\r\n    function createLineItem() {\r\n      fetch(`http://localhost:5000/invoices/${props.id}`, {\r\n        method: \"POST\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\"\r\n        },\r\n        body: JSON.stringify({\r\n          description: state.description,\r\n          quantity: Number(state.quantity),\r\n          cost: Number(state.cost)\r\n        })\r\n      }).then(fetchLineItems)\r\n    }\r\n\r\n    onMounted(fetchLineItems)\r\n\r\n    return {state, createLineItem}\r\n  }\r\n})\r\n</script>"],"sourceRoot":""}]}